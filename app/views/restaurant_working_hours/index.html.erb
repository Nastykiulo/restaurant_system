<p id="notice"><%= notice %></p>

<h1>Restaurant Working Hours</h1>

<table>
  <thead>
    <tr>
      <th>Restaurant</th>
      <th>Day</th>
      <th>Opening time</th>
      <th>Closing time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurant_working_hours.each do |restaurant_working_hour| %>
      <tr>
        <td><%= restaurant_working_hour.restaurant %></td>
        <td><%= restaurant_working_hour.day %></td>
        <td><%= restaurant_working_hour.opening_time %></td>
        <td><%= restaurant_working_hour.closing_time %></td>
        <td><%= link_to 'Show', restaurant_working_hour %></td>
        <td><%= link_to 'Edit', edit_restaurant_working_hour_path(restaurant_working_hour) %></td>
        <td><%= link_to 'Destroy', restaurant_working_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restaurant Working Hour', new_restaurant_working_hour_path %>
